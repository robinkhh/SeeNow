            @Html.Action("TopNav", "Home", new { role_type = Session["user_role"] })\
            @Html.Action("UserNav", "Home", new { role_type = Session["user_role"] })
        </div>
    </nav>
    @*<div class="container ">*@
        @RenderBody()
        @*<hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - SeeNow</p>
        </footer>*@
    @*</div>*@

